@model FeedbackResponseModel;
<div class="container">
    <form asp-controller="Feedback" asp-action="Edit" method="post">
        <input type="hidden" asp-for="Id" />
        <div class="my-5 row">
            <label for="InterviewId">InterviewId</label>
            <input asp-for="InterviewId" class="form-control" />
            <span asp-validation-for="InterviewId"></span>
        </div>
        <div class="my-5 row col-12">
            <label for="Description">Description</label>
            <input asp-for="Description" class="form-control" />
            <span asp-validation-for="Description"></span>
        </div>
        <div class="my-5 row col-12">
            <label for="Hired">Hired</label>
            <input asp-for="Hired" class="form-control" />
            <span asp-validation-for="Hired"></span>
        </div>
        <input type="submit" value="Update Feedback" class="btn btn-primary" />
    </form>
</div>


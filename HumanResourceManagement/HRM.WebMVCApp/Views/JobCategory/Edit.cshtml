@model JobCategoryResponseModel;
<div class="container">
    <form asp-controller="JobCategory" asp-action="Edit" method="post">
        <input type="hidden" asp-for="Id" />
        <div class="my-5 row">
            <label for="Name">Name</label>
            <input asp-for="Name" class="form-control" />
            <span asp-validation-for="Name"></span>
        </div>
        <div class="my-5 row col-12">
            <label for="IsActive">IsActive</label>
            <input asp-for="IsActive" class="form-control" />
            <span asp-validation-for="IsActive"></span>
        </div>
        <input type="submit" value="Update JobCategory" class="btn btn-primary" />
    </form>
</div>


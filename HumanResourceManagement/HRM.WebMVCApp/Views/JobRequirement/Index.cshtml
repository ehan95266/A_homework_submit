@model IEnumerable<JobRequirementResponseModel>;
<div class="page-header">
    <h1>List of Job Requirements</h1>
</div>
<a asp-action="Create" asp-controller="JobRequirement">Add New Job Requirement</a>
<table class="table table-bordered table-striped table-hovered">
    <thead>
        <tr>
            <th>Title</th>
            <th>Is Active</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var m in Model)
            {
                <tr>
                    <td>@m.Title</td>
                    <td>@m.IsActive</td>
                    <td><a asp-action="Edit" asp-controller="JobRequirement" asp-route-id="@m.Id">Edit</a></td>
                    <td><a asp-action="Delete" asp-controller="JobRequirement" asp-route-id="@m.Id">Delete</a></td>
                </tr>
            }
        }
    </tbody>
</table>

@model InterviewResponseModel;
<form asp-action="Delete" asp-controller="Interview" method="post">
    <div class="container">
        <div class="row">
            <label asp-for="SubmissionId"></label>
            <span>@Model.SubmissionId</span>
        </div>
        <div class="row">
            <label asp-for="InterviewDate"></label>
            <span>@Model.InterviewDate</span>
        </div>

        <input type="submit" value="Delete" class="btn btn-danger" />
    </div>
</form>

